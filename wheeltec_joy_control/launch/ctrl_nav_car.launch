<launch>
    <!--打开小海龟画面-->
    <!-- <node pkg="turtlesim" type="turtlesim_node" name="turtlesim_node"/> -->
    <!--获取手柄数据节点-->
    <node  pkg="joy" type="joy_node" name="joystick">
       <param name="autorepeat_rate"      type="double"   value="10"/>
       <param name="dev_ff"      type="str"   value="/dev/input/event5"/>
    </node> 
    <!--打开手柄控制节点-->
    <node pkg="teleop_twist_joy" type="teleop_node" name="joy_control" output="screen">
       <rosparam command="load" file="$(find teleop_twist_joy)/config/ps3.config.yaml" />
       <param name="enable_button"      type="int"   value="4"/>
       <param name="axis_angular"      type="int"   value="3"/>
       <param name="enable_turbo_button"      type="int"   value="5"/>
       <remap from="/cmd_vel" to="/turtle1/cmd_vel" />
    </node>
</launch>
